{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react"],
    "rules": {
      //add customize rules here as per your project's needs
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "indent": ["warn", 2],
        "quotes": ["warn", "double"],
        "jest/valid-expect": 0
    }
}  